            <TextBox x:Name="txtAncho1" TextWrapping="Wrap" Margin="165,176,28,446" FontSize="36" Height="47" IsReadOnly="True"/>
            <TextBox x:Name="txtLargo1" TextWrapping="Wrap" Margin="165,117,28,505" FontSize="36" Height="47" IsReadOnly="True"/>
            <TextBox x:Name="txtAlto_Copy1" TextWrapping="Wrap" Margin="165,55,28,567" FontSize="36" Height="47" IsReadOnly="True"/>
            <Label x:Name="lblAlto_Copy1" Content="# Piezas" HorizontalAlignment="Left" Margin="33,299,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.281,0.551" Width="117" FontSize="28" Height="47"/>
            <TextBox x:Name="txtPiezas" TextWrapping="Wrap" Margin="165,299,28,323" FontSize="24" IsReadOnly="True"/>
