            <Label x:Name="lblAlto1" Content="Alto :" HorizontalAlignment="Left" Margin="33,55,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.281,0.551" Width="117" FontSize="28"/>
            <Label x:Name="lblAlto_Copy5" Content="Largo :" HorizontalAlignment="Left" Margin="33,117,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.281,0.551" Width="117" FontSize="28"/>
            <Label x:Name="lblAlto_Copy6" Content="Ancho :" HorizontalAlignment="Left" Margin="33,176,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Width="117" FontSize="28"/>
