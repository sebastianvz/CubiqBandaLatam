            <Button x:Name="btnbuscarWAM" Content="Buscar" HorizontalAlignment="Left" VerticalAlignment="Top" Width="112" Margin="339,10,0,0" Height="47" FontSize="24" Click="BtnbuscarWAM_Click"/>
            <Grid x:Name="gridHabuttons" HorizontalAlignment="Left" Height="517" VerticalAlignment="Top" Width="433" Margin="18,142,0,0">
            </Grid>
        </Grid>
