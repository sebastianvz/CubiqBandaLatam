        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1280">
    <Grid RenderTransformOrigin="0.273,0.455" Height="689" VerticalAlignment="Bottom">
        <Grid HorizontalAlignment="Left" Height="669" VerticalAlignment="Top" Width="397" Margin="0,10,0,0">
